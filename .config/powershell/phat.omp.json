{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#b3ffde",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }}",
          "type": "session"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ffdd86",
          "options": {
            "fetch_status": true,
            "branch_icon": ""
          },
          "template": " git:{{ .HEAD }}{{if .BranchStatus }} <#B388FF>{{ .BranchStatus }}</>{{ end }}{{ if .Staging.Changed }} <#98c379>{{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}{{ end }}{{ if .Working.Changed }} <#d16971>{{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffdd86>{{ .StashCount }}{{ end }}"        
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "foreground": "#b3ffde",
          "options": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": "<b3ffde>{{ .FormattedMs }} - </>"
        },
        {
          "type": "time",
          "foreground": "#b3ffde",
          "options": {
            "time_format": "02/01/06"
          },
          "style": "plain",
          "template": "<b3ffde>[{{ .CurrentDate | date .Format }}</>"
        },
        {
          "type": "time",
          "foreground": "#b3ffde",
          "options": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "<b3ffde> - {{ .CurrentDate | date .Format }}]</>"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "foreground": "#61AFEF",
          "options": {
            "style": "full"
          },
          "style": "plain",
          "template": "<#61AFEF>\ue5ff {{ .Path }}</>"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u0021"
        },
        {
          "type": "text",
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u276f"
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#5C6370",
    "template": "‚ùØ "
  },
  "final_space": true,
  "upgrade": {
    "interval": "168h",
    "auto": false,
    "source": "cdn"
  },
  "version": 4
}